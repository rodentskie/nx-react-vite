{
  "name": "library-link.group",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "library/link.group/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["library/link.group/**/*.{ts,tsx,js,jsx}"]
      }
